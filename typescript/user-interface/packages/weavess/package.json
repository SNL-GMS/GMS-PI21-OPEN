{
  "name": "@gms/weavess",
  "version": "1.3.0",
  "description": "WEAVESS: Web Enhanced Application for the Viewing & Exploitation of Sensor Samples, a library for high-performance waveform display",
  "license": "UNLICENSED",
  "author": "GMS Development Team",
  "sideEffects": false,
  "module": "./lib/weavess.js",
  "typings": "./lib/weavess.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean:node_modules": "yarn rimraf node_modules/",
    "clean:build": "yarn rimraf lib/ dist/ docs/ coverage/ bundle-analyzer/ artifacts/ .jest-cache/ .tsbuildinfo .tsbuildinfo-test .eslintcache .eslintcache-test junit.xml",
    "clean": "npm-run-all -s clean:build clean:node_modules",
    "build:eslint": "yarn eslint -c .eslintrc-config.yaml 'src/**/*.{ts,tsx}' --quiet --cache --cache-file .eslintcache",
    "build:eslint:test": "yarn eslint -c .eslintrc-config-test.yaml '{__tests__,__mocks__,__integration-tests__}/**/*.{ts,tsx}' --quiet --cache --cache-file .eslintcache-test",
    "build": "yarn tsc -b ./tsconfig-build.json",
    "build:test": "yarn tsc -b ./tsconfig-test.json",
    "watch": "yarn tsc -b ./tsconfig-build.json -watch",
    "dev": "webpack serve --color --progress --env development --env devserver --config webpack.config.ts",
    "start": "webpack serve --color --progress --env production --env devserver --config webpack.config.ts",
    "docs": "yarn clean:docs && typedoc --out docs ./src/ --name weavess --mode file --readme README.md --target ES6 --includeDeclarations --excludeExternals --excludeNotExported --hideGenerator",
    "sonar": "echo \"run 'yarn sonar' from the top-level directory\"",
    "test": "yarn test:dev",
    "test:dev:jest": "jest --config jest.config.js --maxWorkers=50% --verbose --bail",
    "test:prod:jest": "jest --config jest.config.js --maxWorkers=4 --silent --bail --coverageReporters=lcovonly --reporters=default --reporters=jest-junit",
    "test:dev": "npm-run-all -s build:test test:dev:jest",
    "test:prod": "npm-run-all -s build:test test:prod:jest",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "@blueprintjs/core": "4.2.1",
    "@blueprintjs/datetime": "4.2.0",
    "@blueprintjs/icons": "4.2.5",
    "@blueprintjs/popover2": "1.1.4",
    "@gms/common-util": "0.1.0",
    "@gms/ui-util": "0.1.0",
    "@gms/weavess-core": "0.1.0",
    "@gms/webpack-config": "0.1.0",
    "classnames": "2.2.6",
    "d3": "5.5.0",
    "element-resize-event": "3.0.3",
    "immutable": "4.0.0-rc.12",
    "lodash": "4.17.21",
    "memoize-one": "5.1.1",
    "moment": "2.26.0",
    "moment-precise-range-plugin": "1.3.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hotkeys-hook": "3.4.6",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-toastify": "9.0.1",
    "resize-observer-polyfill": "1.5.1",
    "three": "0.98.0",
    "worker-loader": "2.0.0",
    "worker-rpc": "0.2.0"
  },
  "devDependencies": {
    "@blueprintjs/eslint-plugin": "2.2.0",
    "@cfaester/enzyme-adapter-react-18": "0.5.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.4.3",
    "@types/classnames": "2.2.9",
    "@types/d3": "4.12.0",
    "@types/enzyme": "3.10.12",
    "@types/faker": "4.1.8",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.149",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "copyfiles": "2.1.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-utils": "1.12.1",
    "enzyme-to-json": "3.4.3",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.1",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.3",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "43.0.2",
    "eslint-plugin-workspaces": "0.7.0",
    "faker": "4.1.0",
    "jest": "28.1.0",
    "jest-canvas-mock": "2.4.0",
    "jest-cli": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-fetch-mock": "3.0.3",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "react-test-renderer": "18.1.0",
    "rimraf": "3.0.0",
    "ts-jest": "28.0.1",
    "ts-node": "9.1.1",
    "typedoc": "0.15.3",
    "typescript": "4.6.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.1"
  },
  "peerDependencies": {
    "worker-loader": "2.0.0"
  },
  "jest-junit": {
    "suiteNameTemplate": "weavess/{filename}",
    "classNameTemplate": "weavess/{filename}/{classname}",
    "titleTemplate": "{title}"
  }
}
