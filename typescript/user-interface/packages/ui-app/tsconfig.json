{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "typeRoots": ["@types", "./@types", "./node_modules/@types", "../../node_modules/@types"],
    "strictFunctionTypes": false,
    "baseUrl": ".",
    "noEmit": true,
    "outDir": "./lib",
    "paths": {
      "~analyst-ui": ["./src/ts/components/analyst-ui"],
      "~analyst-ui/": ["./src/ts/components/analyst-ui"],
      "~analyst-ui/*": ["./src/ts/components/analyst-ui/*"],
      "~app": ["./src/ts/app"],
      "~app/": ["./src/ts/app"],
      "~app/*": ["./src/ts/app/*"],
      "~config": ["./src/ts/config"],
      "~config/": ["./src/ts/config"],
      "~config/*": ["./src/ts/config/*"],
      "~env": ["./src/ts/env"],
      "~env/": ["./src/ts/env"],
      "~env/*": ["./src/ts/env/*"],
      "~common-ui": ["./src/ts/components/common-ui"],
      "~common-ui/": ["./src/ts/components/common-ui"],
      "~common-ui/*": ["./src/ts/components/common-ui/*"],
      "~components": ["./src/ts/components"],
      "~components/": ["./src/ts/components"],
      "~components/*": ["./src/ts/components/*"],
      "~css": ["./src/css"],
      "~css/": ["./src/css"],
      "~css/*": ["./src/css/*"],
      "~data-acquisition-ui": ["./src/ts/components/data-acquisition-ui"],
      "~data-acquisition-ui/": ["./src/ts/components/data-acquisition-ui"],
      "~data-acquisition-ui/*": ["./src/ts/components/data-acquisition-ui/*"],
      "~resources": ["./src/ts/resources"],
      "~resources/": ["./src/ts/resources"],
      "~resources/*": ["./src/ts/resources/*"],
      "~scss-config": ["./src/ts/scss-config"],
      "~scss-config/": ["./src/ts/scss-config"],
      "~scss-config/*": ["./src/ts/scss-config/*"],
      "@gms/common-model": ["../common-model/src/ts/common-model.ts"],
      "@gms/common-model/lib/*": ["../common-model/src/ts/*"],
      "@gms/common-util": ["../common-util/src/ts/common-util.ts"],
      "@gms/common-util/lib/*": ["../common-util/src/ts/*"],
      "@gms/ui-core-components": ["../ui-core-components/src/ts/ui-core-components.ts"],
      "@gms/ui-core-components/lib/*": ["../ui-core-components/src/ts/*"],
      "@gms/ui-state": ["../ui-state/src/ts/ui-state.ts"],
      "@gms/ui-state/lib/*": ["../ui-state/src/ts/*"],
      "@gms/ui-workers": ["../ui-workers/src/ts/ui-workers.ts"],
      "@gms/ui-workers/lib/*": ["../ui-workers/src/ts/*"],
      "@gms/ui-util": ["../ui-util/src/ts/ui-util.ts"],
      "@gms/ui-util/lib/*": ["../ui-util/src/ts/*"],
      "@gms/weavess": ["../weavess/src/ts/weavess.ts"],
      "@gms/weavess/lib/*": ["../weavess/src/ts/*"],
      "@gms/weavess-core": ["../weavess-core/src/ts/weavess-core.ts"],
      "@gms/weavess-core/lib/*": ["../weavess-core/src/ts/*"],
      "@gms/webpack-config": ["../webpack-config/src/ts/webpack-config.ts"],
      "@gms/webpack-config/lib/*": ["../webpack-config/src/ts/*"]
    }
  },
  "references": [
    {
      "path": "../common-model/tsconfig-build.json"
    },
    {
      "path": "../common-util/tsconfig-build.json"
    },
    {
      "path": "../ui-core-components/tsconfig-build.json"
    },
    {
      "path": "../ui-state/tsconfig-build.json"
    },
    {
      "path": "../ui-workers/tsconfig-build.json"
    },
    {
      "path": "../ui-util/tsconfig-build.json"
    },
    {
      "path": "../weavess/tsconfig-build.json"
    },
    {
      "path": "../weavess-core/tsconfig-build.json"
    },
    {
      "path": "../webpack-config"
    }
  ],
  "include": ["src/**/*", "__tests__/**/*", "__mocks__/**/*"]
}
