{
  "name": "@gms/webpack-config",
  "version": "0.1.0",
  "description": "GMS Webpack Build Scripts",
  "license": "UNLICENSED",
  "author": "GMS Development Team",
  "sideEffects": false,
  "main": "./src/ts/webpack-config.ts",
  "typings": "./lib/webpack-config.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean:node_modules": "yarn rimraf node_modules/",
    "clean:build": "yarn rimraf lib/ dist/ docs/ coverage/ bundle-analyzer/ artifacts/ .jest-cache/ .tsbuildinfo .tsbuildinfo-test .eslintcache .eslintcache-test junit.xml",
    "clean": "npm-run-all -s clean:build clean:node_modules",
    "build:eslint": "yarn eslint -c .eslintrc-config.yaml 'src/**/*.{ts,tsx}' --quiet --cache --cache-file .eslintcache",
    "build": "yarn tsc -b ./tsconfig.json",
    "watch": "yarn tsc -b ./tsconfig.json -watch",
    "docs": "yarn clean:docs && typedoc --out docs ./src/ --name webpack-config --mode file --readme README.md --target ES6 --includeDeclarations --excludeExternals --excludeNotExported --hideGenerator",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "fork-ts-checker-webpack-plugin": "7.2.7",
    "git-revision-webpack-plugin": "5.0.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.0",
    "node-polyfill-webpack-plugin": "1.1.4",
    "resolve-url-loader": "5.0.0",
    "sass": "1.49.9",
    "sass-loader": "12.6.0",
    "script-loader": "0.7.2",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "transform-loader": "0.2.4",
    "ts-loader": "9.2.8",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.1",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {
    "@types/case-sensitive-paths-webpack-plugin": "2.1.6",
    "@types/sass-loader": "8.0.3",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@types/webpack-node-externals": "2.5.3",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.1",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.3",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "43.0.2",
    "eslint-plugin-workspaces": "0.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "rimraf": "3.0.0",
    "ts-node": "9.1.1",
    "typedoc": "0.15.3",
    "typescript": "4.6.4"
  }
}
