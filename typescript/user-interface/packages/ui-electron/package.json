{
  "name": "@gms/ui-electron",
  "version": "0.1.0",
  "description": "GMS UI Electron - window management for GMS analyst UI",
  "license": "UNLICENSED",
  "author": "GMS Development Team",
  "main": "./dist/ui-electron.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:node_modules": "yarn rimraf node_modules/",
    "clean:dist": "yarn rimraf dist/",
    "clean:build": "yarn rimraf lib/ dist/ docs/ coverage/ bundle-analyzer/ artifacts/ .jest-cache/ .tsbuildinfo .tsbuildinfo-test .eslintcache .eslintcache-test junit.xml",
    "clean": "npm-run-all -s clean:build clean:node_modules",
    "build:eslint": "yarn eslint -c .eslintrc-config.yaml 'src/**/*.{ts,tsx}' --quiet --cache --cache-file .eslintcache",
    "build:eslint:test": "yarn eslint -c .eslintrc-config-test.yaml '{__tests__,__mocks__,__integration-tests__}/**/*.{ts,tsx}' --quiet --cache --cache-file .eslintcache-test",
    "build": "yarn tsc -b ./tsconfig-build.json",
    "build:test": "yarn tsc -b ./tsconfig-test.json",
    "watch": "yarn tsc -b ./tsconfig-build.json -watch",
    "build:webpack:dev": "webpack --color --env development --config webpack.config.ts",
    "build:webpack:prod": "webpack --color --env production --config webpack.config.ts",
    "bundle:dev": "npm-run-all -p clean:dist -p build:webpack:dev",
    "bundle:prod": "npm-run-all -p clean:dist -p build:webpack:prod",
    "bundle": "yarn bundle:dev",
    "dev": "yarn bundle:dev && electron ./dist/ui-electron.js",
    "start": "yarn bundle:prod && electron ./dist/ui-electron.js",
    "docs": "yarn clean:docs && typedoc --out docs ./src/ --name ui-electron --mode file --readme README.md --target ES6 --includeDeclarations --excludeExternals --excludeNotExported --hideGenerator",
    "sonar": "echo \"run 'yarn sonar' from the top-level directory\"",
    "test": "yarn test:dev",
    "test:dev:jest": "jest --config jest.config.js --maxWorkers=50% --verbose --bail",
    "test:prod:jest": "jest --config jest.config.js --maxWorkers=4 --silent --bail --coverageReporters=lcovonly --reporters=default --reporters=jest-junit",
    "test:dev": "npm-run-all -s build:test test:dev:jest",
    "test:prod": "npm-run-all -s build:test test:prod:jest",
    "version": "echo $npm_package_version",
    "generate-bin": "yarn build && yarn generate-darwin-x64 && yarn generate-win32-x64",
    "generate-darwin-x64": "electron-packager . --platform=darwin --arch=x64 --icon=gms-logo-favicon.icns --out dist --no-package-manager",
    "generate-win32-x64": "electron-packager . --platform=win32 --arch=x64 --icon=gms-logo-favicon.ico --out dist --no-package-manager"
  },
  "dependencies": {
    "@blueprintjs/core": "4.2.1",
    "@gms/common-util": "0.1.0",
    "@gms/ui-state": "0.1.0",
    "@gms/ui-util": "0.1.0",
    "@gms/webpack-config": "0.1.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@blueprintjs/eslint-plugin": "2.2.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@types/faker": "4.1.8",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.149",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "electron": "19.0.8",
    "electron-context-menu": "3.3.0",
    "electron-json-storage": "4.5.0",
    "electron-packager": "15.5.1",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.1",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.3",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "43.0.2",
    "eslint-plugin-workspaces": "0.7.0",
    "faker": "4.1.0",
    "jest": "28.1.0",
    "jest-canvas-mock": "2.4.0",
    "jest-cli": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-fetch-mock": "3.0.3",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "lodash": "4.17.21",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "redux": "4.1.2",
    "redux-electron-store": "0.6.6",
    "rimraf": "3.0.0",
    "ts-jest": "28.0.1",
    "ts-node": "9.1.1",
    "typedoc": "0.15.3",
    "typescript": "4.6.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "jest-junit": {
    "suiteNameTemplate": "ui-electron/{filename}",
    "classNameTemplate": "ui-electron/{filename}/{classname}",
    "titleTemplate": "{title}"
  },
  "productName": "GMS Interactive Analysis"
}
